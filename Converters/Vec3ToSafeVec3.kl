require Math;

/// \dfgPresetFolder Converters
/// Returns a new Vec3 with NaNs removed.
function Vec3 Vec3ToSafeVec3(in Vec3 this) {
  Vec3 result();
  if(this.x.isNaN())
    result.x = 0.0;
  else
  	result.x = this.x;
  if(this.y.isNaN())
    result.y = 0.0;
  else
  	result.y = this.y;
  if(this.z.isNaN())
    result.z = 0.0;
  else
  	result.z = this.z;
  return result;
}
