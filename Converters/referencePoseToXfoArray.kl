require Characters;

/// \dfgPresetFolder Converters/Pose
/// Gets the skeleton's reference pose into an array of Xfos.
function Xfo[] referencePoseToXfoArray(
  in ISkeleton this
) {
  Xfo result[];
  for(Index i=0; i<this.getNumBones(); i++) {
    result.push(this.getReferencePose(i));
  }
  return result;
}
